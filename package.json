{
  "name": "sentry-js-sdk",
  "version": "1.0.0",
  "description": "A generic Javascript SDK based on Self-Hosted Sentry.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.common.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "rollup -c --watch",
    "build": "rollup -c",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmoyDreamer/sentry-js-sdk.git"
  },
  "keywords": [
    "sentry"
  ],
  "author": "Allen Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmoyDreamer/sentry-js-sdk/issues"
  },
  "homepage": "https://github.com/AmoyDreamer/sentry-js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "husky": "^8.0.3",
    "rollup": "^3.14.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "error-stack-parser": "^2.1.4"
  }
}
